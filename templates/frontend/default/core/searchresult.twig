{% extends parent_template %}

{% block title %}{{ Lang.get('core.search_results') }}{% endblock %}

{% block breadcrumb %}
    {{ parent() }}
    {{ Lang.get('core.search_results') }}
{% endblock %}

{% block content %}

    {{ Lang.get('core.found_results', { 'total': articles|length }) }} '<strong>{{ Input.get('query') }}</strong>'.

    {% if articles is not empty %}
        <hr />

        {% include 'frontend.' ~ template ~ '.selfservice.article_list' %}
    {% endif %}

{% endblock %}